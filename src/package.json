{
  "name": "alfred.gui",
  "version": "1.0.0",
  "description": "This is alfred's ui using angular",
  "main": "./dist/main.js",
  "scripts": {
    "test:node": "mocha-webpack --watch --webpack-config config/webpack.test.node.js --require config/mocha-test-shim.node.js \"app/**/*.spec.ts\"",
    "test:browser": "node tasks/test",
    "report": "nyc mocha-webpack --webpack-config config/webpack.test.node.js --require config/mocha-test-shim.node.js \"app/**/*.spec.ts\"",
    "coverage": "nyc report --reporter=html",
    "remap": "remap-istanbul -i .nyc_output/71ff96c6c4d270697bd14dfd4d543734.json -o coverage/remap-report -t html -e node_modules",
    "start": "node tasks/debug",
    "prerelease": "rm -rf dist",
    "release": "node tasks/release",
    "ngc-w": "./node_modules/.bin/ngc-w -p tsconfig-aot.json",
    "compile:aot": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "prebuild:aot": "rm -rf aot",
    "build:aot": "npm run ngc-w -- --webpack config/webpack.config.prd.js && webpack --config config/webpack.config.prd.js  --progress --profile --bail",
    "prerelease:aot": "npm run build:aot",
    "release:aot": "node_modules/.bin/browser-sync start --server aot --files aot"
  },
  "keywords": [
    "npm"
  ],
  "author": "Bilel Msekni <msekni.bilel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.39",
    "@types/node": "^6.0.63",
    "angular-router-loader": "^0.4.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.3",
    "browser-sync": "^2.18.8",
    "bs-fullscreen-message": "^1.1.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.0",
    "mocha-webpack": "^0.7.0",
    "ngc-webpack": "^1.2.0",
    "null-loader": "^0.1.1",
    "nyc": "^10.1.2",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.9.1",
    "style-loader": "^0.13.1",
    "ts-node": "^2.1.0",
    "tslint": "^3.15.1",
    "typescript": "^2.2.1",
    "wallaby-webpack": "0.0.34",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-merge": "^2.6.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angular/upgrade": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "angular2-highcharts": "^0.4.3",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.1.1",
    "core-js": "^2.4.1",
    "datatables": "^1.10.13",
    "drmonty-datatables-plugins": "^1.10.12",
    "drmonty-datatables-responsive": "^2.1.0",
    "flot": "^0.8.0-alpha",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "metismenu": "^2.6.2",
    "moment": "^2.17.1",
    "morris.js": "^0.5.0",
    "raphael": "^2.2.7",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.1.1",
    "zone.js": "^0.8.5"
  },
  "nyc": {
    "include": [
      "app/*.ts",
      "app/**/*.ts"
    ],
    "exclude": [
      "app/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
