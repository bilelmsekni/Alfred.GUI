  "main": "./dist/main.js",
  "scripts": {
    "test": "nyc mocha-webpack --webpack-config config/webpack.config.test.js \"app/**/*.test.ts\"",
    "coverage": "nyc report --reporter=text-lcov",
    "start": "node tasks/debug",
    "prerelease": "rm -rf aot",
    "release": "node tasks/release",
    "ngc": "./node_modules/.bin/ngc-w -p tsconfig-aot.json",
    "prebuild:aot": "rm -rf aot",
    "build:aot": "npm run ngc -- --webpack config/webpack.config.prd.js && webpack --config config/webpack.config.prd.js  --progress --profile --bail"
